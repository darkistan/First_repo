using System;

class Program
{
    static void Main()
    {
        // Виправлено:
        // - Ініціалізацію змінної sum (тепер int sum = 0;)
        // - Умова циклу: i < numbers.Length (щоб не виходити за межі масиву)
        // - Перевірку на парність: використав numbers[i] % 2 == 0 (правильний оператор порівняння '==')
        // - Накопичення суми: використав sum += numbers[i];
        // - Вивід: правильно з'єднав рядок і змінну через " + sum"
        int[] numbers = { 1, 2, 3, 4, 5, 6, 8, 11 };
        int sum = 0; // Ініціалізував sum нулем перед використанням

        for (int i = 0; i < numbers.Length; i++) // Умова i < numbers.Length — щоб не було виходу за межі
        {
            // Перевіряю, чи є число парним — оператор '%' та '=='
            if (numbers[i] % 2 == 0)
            {
                // Додаю парне число до суми за допомогою +=
                sum += numbers[i];
            }
        }

        
        Console.WriteLine("Сума парних чисел = " + sum);
    }
}
